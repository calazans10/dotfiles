[apply]
  whitespace = nowarn
[color]
  branch = auto
  diff = auto
  status = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[branch]
  autosetupmerge = true
[core]
  editor = subl -w
  autocrlf = input
  safecrlf = true
  excludesfile = /home/calazans/.gitignore
[push]
  default = current
[alias]
  lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
  who = shortlog -s --
  url =! bash -c 'git config --get remote.origin.url | sed -E "s/.+:\\(.+\\)\\.git$/https:\\\\/\\\\/github\\\\.com\\\\/\\\\1/g"'
[help]
  autocorrect = 1
[user]
  name = Jeferson Farias Calazans
  email = calazans10@gmail.com
[github]
  user = calazans10
[giggle]
  main-window-maximized = true
  main-window-geometry = 731x449+0+24
  history-view-vpane-position = 469
  file-view-vpane-position = 700
  main-window-view = HistoryView
