[apply]
  whitespace = nowarn
[color]
  branch = auto
  diff = auto
  status = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[branch]
  autosetupmerge = true
[core]
  editor = subl -w
  autocrlf = input
  safecrlf = true
  excludesfile = /Users/calazans/.gitignore
[push]
  default = current
[alias]
  lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
  who = shortlog -s --
[help]
  autocorrect = 1
[user]
  name = Jeferson Farias Calazans
  email = calazans10@gmail.com
[github]
  user = calazans10
